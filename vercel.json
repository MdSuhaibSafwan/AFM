{
    "builds": [
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles"
        }
      },
      {
        "src": "AFM/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "150mb", "runtimee": "python3.9"}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "AFM/wsgi.py"
      }
    ]
}